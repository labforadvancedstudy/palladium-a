fn main() {
    // Test arrays
    let mut scores: [i64; 5] = [85, 92, 78, 95, 88];
    
    print("Student Test Scores:");
    print_int(scores[0]);
    print_int(scores[1]);
    print_int(scores[2]);
    print_int(scores[3]);
    print_int(scores[4]);
    
    // Modify array
    scores[2] = 90;
    
    print("After grade adjustment:");
    print_int(scores[2]);
    
    // Calculate sum using while loop
    let mut i = 0;
    let mut sum = 0;
    while (i < 5) {
        sum = sum + scores[i];
        i = i + 1;
    }
    
    print("Total sum:");
    print_int(sum);
    
    // Test if/else
    if (sum > 400) {
        print("Class performance: Excellent!");
    } else {
        print("Class performance: Needs improvement");
    }
    
    print("Demo completed!");
}