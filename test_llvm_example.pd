// Test LLVM backend with a simple example
fn add(a: i64, b: i64) -> i64 {
    return a + b;
}

fn fibonacci(n: i64) -> i64 {
    if n <= 1 {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

fn main() {
    let x = 10;
    let y = 20;
    let sum = add(x, y);
    print_int(sum);
    
    let fib = fibonacci(10);
    print_int(fib);
}