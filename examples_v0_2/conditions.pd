// If/else and comparisons example
fn main() -> i32 {
    let x = 10;
    let y = 20;
    
    print("x = ");
    print_int(x);
    print("y = ");
    print_int(y);
    
    if x < y {
        print("x is less than y");
    } else {
        print("x is not less than y");
    }
    
    // Find maximum (assignment not yet supported, using function call instead)
    if x > y {
        print("The maximum is: ");
        print_int(x);
    } else {
        print("The maximum is: ");
        print_int(y);
    }
    
    // Nested if
    let age = 25;
    if age >= 18 {
        print("Adult");
        if age >= 65 {
            print("Senior");
        } else {
            print("Not a senior");
        }
    } else {
        print("Minor");
    }
    
    return 0;
}