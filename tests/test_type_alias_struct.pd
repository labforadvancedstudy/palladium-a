// Test type aliases with structs
struct Point {
    x: i32,
    y: i32,
}

type Coordinate = Point;
type Coordinates = [Point; 10];

fn distance(p1: Coordinate, p2: Coordinate) -> i32 {
    let dx = p2.x - p1.x;
    let dy = p2.y - p1.y;
    // Just return the sum for testing - not a real distance
    return dx + dy;
}

fn main() {
    let origin: Coordinate = Point { x: 0, y: 0 };
    let target: Coordinate = Point { x: 3, y: 4 };
    
    let dist = distance(origin, target);
    print("Distance: ");
    print_int(dist);
}