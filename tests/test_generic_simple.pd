// Simple test for generic array inference

fn first<T>(arr: [T; 3]) -> T {
    return arr[0];
}

fn main() {
    // Test with integers
    let nums = [1, 2, 3];
    let n = first(nums);
    print_int(n);
    
    // Test with booleans  
    let bools = [true, false, true];
    let b = first(bools);
    if b {
        print("First bool is true");
    }
    
    print("Done");
}