// Minimal collections test

fn main() -> i64 {
    // Test array indexing
    let arr = [10, 20, 30, 40, 50];
    
    if arr[0] != 10 {
        return 1;
    }
    
    if arr[2] != 30 {
        return 2;
    }
    
    if arr[4] != 50 {
        return 3;
    }
    
    // Test counter (simulating vec length)
    let mut count = 0;
    count = count + 1;  // push
    count = count + 1;  // push
    count = count + 1;  // push
    
    if count != 3 {
        return 4;
    }
    
    count = count - 1;  // pop
    
    if count != 2 {
        return 5;
    }
    
    // All tests passed
    return 0;
}