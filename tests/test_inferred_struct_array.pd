// Test that array literal type inference should work for structs

struct Point {
    x: i64,
    y: i64,
}

fn main() {
    print("Testing struct array inference...\n");
    
    // Create points
    let p1 = Point { x: 10, y: 20 };
    let p2 = Point { x: 30, y: 40 };
    let p3 = Point { x: 50, y: 60 };
    
    // This should work without type annotation
    let points = [p1, p2, p3];
    
    print("Array created successfully!");
    print("\nFirst point x:");
    print_int(points[0].x);
    print("\nFirst point y:");
    print_int(points[0].y);
}