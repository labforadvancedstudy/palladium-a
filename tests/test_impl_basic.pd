// Basic impl test without traits

struct Point {
    x: i32,
    y: i32,
}

impl Point {
    fn new(x: i32, y: i32) -> Point {
        return Point { x: x, y: y };
    }
    
    fn display(p: &Point) {
        print("Point { x: ");
        print_int(p.x);
        print(", y: ");
        print_int(p.y);
        print(" }");
    }
}

fn main() {
    let p: Point = Point::new(10, 20);
    Point::display(&p);
}