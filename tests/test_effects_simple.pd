// Simple effect system test

fn pure_add(a: i32, b: i32) -> i32 {
    return a + b;
}

fn io_greet() {
    print("Hello from IO function\n");
}

fn main() {
    let result = pure_add(5, 3);
    print("Pure result: ");
    print_int(result);
    
    io_greet();
    
    print("Effect test complete\n");
}