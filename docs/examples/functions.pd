// 함수 정의와 호출
fn add(a: i64, b: i64) -> i64 {
    return a + b;
}

fn multiply(x: i64, y: i64) -> i64 {
    return x * y;
}

fn greet(name: String) {
    print("Hello, ");
    print(name);
    print("!");
}

fn main() {
    let sum = add(10, 20);
    print("10 + 20 = ");
    print_int(sum);
    
    let product = multiply(6, 7);
    print("6 * 7 = ");
    print_int(product);
    
    greet("Palladium");
}