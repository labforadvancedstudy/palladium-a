// Test multiple module imports

import std::math;
import std::string;

fn main() {
    // Test math functions
    let x = -42;
    print_int(pd_abs(x));
    
    // Test string functions
    let s = "  Hello, World!  ";
    let trimmed = trim(s);
    print(trimmed);
    
    // Test string predicates
    if starts_with(trimmed, "Hello") {
        print("Starts with Hello!");
    }
    
    if ends_with(trimmed, "World!") {
        print("Ends with World!");
    }
    
    if contains(trimmed, ", ") {
        print("Contains a comma and space!");
    }
    
    // Mix math and string
    let num = pd_pow(2, 3);
    let num_str = int_to_string(num);
    let msg = "2^3 = " + num_str;
    print(msg);
}