// Simple generics test

// Generic identity function
pub fn identity<T>(x: T) -> T {
    return x;
}

// Generic pair swap
pub fn swap_pair<T>(a: T, b: T) -> T {
    return b;  // Just return the second one for simplicity
}

fn main() {
    // Test with integers
    let x: i64 = 42;
    let y = identity(x);
    print_int(y);
    
    // Test swap with integers  
    let a = 10;
    let b = 20;
    let result = swap_pair(a, b);
    print_int(result);
    
    // Test with strings
    let s1 = "Hello";
    let s2 = identity(s1);
    print(s2);
    
    print("Generic functions are working!");
}