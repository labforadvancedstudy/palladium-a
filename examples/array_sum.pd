// Calculate sum of array elements using a while loop
fn main() {
    let numbers = [5, 10, 15, 20, 25, 30];
    
    let mut sum = 0;
    let mut i = 0;
    
    // Sum all elements
    while i < 6 {
        sum = sum + numbers[i];
        i = i + 1;
    }
    
    print("Sum of array elements:");
    print_int(sum);
    
    // Find maximum value
    let mut max = numbers[0];
    let mut j = 1;
    
    while j < 6 {
        if numbers[j] > max {
            max = numbers[j];
        }
        j = j + 1;
    }
    
    print("Maximum value:");
    print_int(max);
}