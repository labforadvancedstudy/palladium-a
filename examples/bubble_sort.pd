// Bubble sort implementation in Palladium v0.3
fn main() {
    // Array to sort
    let mut numbers = [64, 34, 25, 12, 22, 11, 90];
    let n = 7;
    
    print("Original array:");
    let mut k = 0;
    while k < n {
        print_int(numbers[k]);
        k = k + 1;
    }
    
    // Bubble sort algorithm
    let mut i = 0;
    while i < n - 1 {
        let mut j = 0;
        while j < n - i - 1 {
            if numbers[j] > numbers[j + 1] {
                // Swap elements
                let temp = numbers[j];
                numbers[j] = numbers[j + 1];
                numbers[j + 1] = temp;
            }
            j = j + 1;
        }
        i = i + 1;
    }
    
    print("Sorted array:");
    k = 0;
    while k < n {
        print_int(numbers[k]);
        k = k + 1;
    }
}